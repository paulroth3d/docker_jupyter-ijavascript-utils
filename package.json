{
  "name": "docker-jupyter-ijavascript-utils",
  "version": "1.21.5",
  "description": "Run Jupyter iJavaScript Utils from within Docker",
  "main": "index.js",
  "scripts": {
    "docker:remove": "docker image rm darkbluestudios/jupyter-ijavascript-utils",
    "docker:package": "npm run docker:package:arm && npm run docker:package:amd",
    "docker:package:arm": "docker build -t darkbluestudios/jupyter-ijavascript-utils:arm_1.21.5 -t darkbluestudios/jupyter-ijavascript-utils:latest --platform linux/arm64/v8 .",
    "docker:package:amd": "docker build -t darkbluestudios/jupyter-ijavascript-utils:amd_1.21.5 -t darkbluestudios/jupyter-ijavascript-utils:binder_1.21.5 --platform linux/amd64 .",
    "docker:run": "docker run -it --rm -p 8888:8888 darkbluestudios/jupyter-ijavascript-utils:latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulroth3d/docker_jupyter-ijavascript-utils.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulroth3d/docker_jupyter-ijavascript-utils/issues"
  },
  "homepage": "https://github.com/paulroth3d/docker_jupyter-ijavascript-utils#readme",
  "dependencies": {
    "d3": "^6.7.0",
    "i18n-iso-countries": "^7.5.0",
    "jupyter-ijavascript-utils": "^1.X.X",
    "node-fetch": "^2.6.7",
    "noisejs": "^2.1.0",
    "sane-topojson": "^4.0.0",
    "topojson-client": "^3.1.0",
    "uuid": "^9.0.0",
    "world-atlas": "^2.0.2"
  }
}
